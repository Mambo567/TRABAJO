    "def multiples(integer,limit):\n",
    "    times = limit/integer\n",
    "    i = times/times\n",
    "    num_list = []\n",
    "    while i <= times:\n",
    "        add_value = integer * i\n",
    "        num_list.append(add_value)\n",
    "        i = i + 1 \n",
    "\n",
    "    print(num_list)\n",
    "    \n",
    "multiples(2,14)\n"
   ]
  }
